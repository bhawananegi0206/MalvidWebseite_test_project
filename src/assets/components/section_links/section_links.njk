			{% inject "assets/components/head/head.njk", {
				header: header,
				subheader: subheader
			} %}

		</div>
	</div>

	<div class="row">
		<div class="column">

			{% inject "assets/components/links/links.njk", links %}
		</div>
		<div class="column align-center">
            <div class="button button--main" id="load-more"  role="button">Load more</div>
		</div>
	</div>

</div>